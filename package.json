{
  "name": "react-todo-backend",
  "version": "1.0.0",
  "description": "API for backnd todoist clone. ",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "babel server --out-dir dist --delete-dir-on-start --source-maps inline --copy-files",
    "dev": "nodemon server --exec babel-node --config .nodemonrc.json | pino-pretty",
    "dev:debug": "nodemon server --exec babel-node --config .nodemonrc.json --inspect | pino-pretty",
    "test": "nyc --reporter=text mocha --require @babel/register --exit",
    "test:debug": "nyc --reporter=text mocha --require @babel/register --inspect-brk --exit",
    "lint": "eslint -c .eslintrc.json {server,test}/**",
    "lint:fix": "eslint --fix -c .eslintrc.json {server,test}/**"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.4.19",
    "nyc": "^14.0.0",
    "pino": "^5.11.3",
    "swagger-express-middleware": "^2.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "mocha": "^6.1.4",
    "mocha-prepare": "^0.1.0",
    "mongodb-memory-server": "^4.2.2",
    "nodemon": "^1.18.10",
    "pino-pretty": "^2.5.0",
    "prettier": "^1.16.4",
    "supertest": "^3.4.2"
  },
  "author": "Carmine DiMascio <cdimascio@gmail.com> (https://github.com/cdimascio)"
}
